@misc{canovas_csam_2016,
	title = {{CSAM}: {Compressed} {SAM} format},
	shorttitle = {Compressed {SAM} format},
	url = {https://academic.oup.com/bioinformatics/article/32/24/3709/2525655},
	abstract = {This paper discusses the challenges of storing and compressing alignment reads generated by next-generation sequencing technologies. It explores different approaches to compress SAM files, focusing on compressing reads and quality fields, which contribute significantly to the file size. The authors introduce a new compressed SAM format, CSAM, that offers efficient storage, supports queries over multiple alignments, and enables random access to the data without the need for decompression.},
	language = {engels},
	publisher = {oxford academic},
	author = {Cánovas, Rodrigo and Moffat, Alistair and Turpin, Turpin},
	month = dec,
	year = {2016},
}

